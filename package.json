{
  "name": "dash_wtgviewer",
  "version": "0.0.36",
  "description": "Viewer for wind turbine simulation results built using threejs for python Dash",
  "repository": {
    "type": "git",
    "url": "git://github.com/thisistheplace/dash-wtgviewer.git"
  },
  "bugs": {
    "url": "https://github.com/thisistheplace/dash-wtgviewer/issues"
  },
  "homepage": "https://github.com/thisistheplace/dash-wtgviewer",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components dash_wtgviewer -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Ben Cannell <bean.can.design@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@react-leaflet/core": "1.0.2",
    "@react-three/drei": "^8.20.2",
    "@react-three/fiber": "^7.0.28",
    "camera-controls": "^1.37.6",
    "chroma-js": "^2.4.2",
    "jquery": "^3.6.3",
    "ramda": "^0.26.1",
    "react-bootstrap": "^2.5.0",
    "react-leaflet": "3.1.0",
    "react-top-loading-bar": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "17.0",
    "react-docgen": "^4.1.1",
    "react-dom": "17.0",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "terser-webpack-plugin": "^2.3.0",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6",
    "webpack-serve": "3.1.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
